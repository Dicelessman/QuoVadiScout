{
  "name": "QuoVadiScout - Strutture & Terreni",
  "short_name": "QuoVadiScout",
  "description": "Sistema avanzato per la gestione di strutture e terreni scout",
  "version": "1.3.0",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "orientation": "portrait-primary",
  "theme_color": "#2f6b2f",
  "background_color": "#f5f6f0",
  "scope": "/",
  "scope_extensions": [
    {"origin": "*.openstreetmap.org"},
    {"origin": "*.firebase.google.com"},
    {"origin": "*.googleapis.com"}
  ],
  "lang": "it",
  "dir": "ltr",
  "categories": ["utilities", "productivity", "lifestyle", "navigation", "social", "business"],
  "handle_links": "preferred",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Aggiungi Struttura",
      "short_name": "Aggiungi",
      "description": "Aggiungi una nuova struttura",
      "url": "/?action=add",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Visualizza statistiche e mappe",
      "url": "/dashboard.html",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Elenco Personale",
      "short_name": "Elenco",
      "description": "Visualizza il tuo elenco personale",
      "url": "/?action=personal",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Download Offline",
      "short_name": "Offline",
      "description": "Gestisci download per uso offline",
      "url": "/?action=offline",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Mappa Strutture",
      "short_name": "Mappa",
      "description": "Visualizza strutture su mappa",
      "url": "/?action=map",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Posizione Corrente",
      "short_name": "GPS",
      "description": "Trova strutture vicino a te",
      "url": "/?action=nearby",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+quovadiscout",
      "url": "/?handler=%s"
    }
  ]
}
